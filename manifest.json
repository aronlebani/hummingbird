{
  "manifest_version": 3,
  "name": "Hummingbird",
  "version": "1.2",
  "description": "A light and zippy bookmarks navigator",
  "icons": {
    "16": "assets/favicon-16x16.png",
    "32": "assets/favicon-32x32.png",
    "128": "assets/favicon-128.png"
  },
  "chrome_url_overrides" : {
    "newtab": "index.html"
  },
  "permissions": [
    "bookmarks"
  ]
}
